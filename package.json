{
  "name": "stark-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate for web applications",
  "scripts": {
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js --mode development --watch --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --mode production --progress",
    "lint:js": "eslint --ext js,jsx --fix",
    "lint:scss": "stylelint ./src/**/*.scss --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "check": "npm-check -u -s"
  },
  "author": "Breno Sena",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-react": "^7.20.6",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.0.0",
    "git-commit-msg-linter": "^2.4.4",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.13.0",
    "npm-check": "^5.9.2",
    "offline-plugin": "^5.0.7",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-font-magician": "^2.3.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^5.1.2",
    "webpack-pwa-manifest": "^4.1.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.2.0"
  }
}
