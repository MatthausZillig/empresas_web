{
  "name": "stark-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate for web applications",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --mode development --watch --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --mode production --progress",
    "lint:js": "eslint ./src/",
    "fix:js": "eslint ./src/ --fix",
    "lint:scss": "stylelint ./src/**/*.scss",
    "fix:scss": "stylelint ./src/**/*.scss --fix",
    "lint": "npm lint:js -- npm lint:scss"
  },
  "author": "Breno Sena",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.5.5",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano-webpack-plugin": "^1.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "offline-plugin": "^5.0.7",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-font-magician": "^2.3.1",
    "postcss-loader": "^3.0.0",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.1",
    "webpack-pwa-manifest": "^4.1.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  }
}
